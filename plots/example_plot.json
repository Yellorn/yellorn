{
  "id": "example_plot",
  "owner": "Yellorn",
  "metadata": {
    "title": "Example Plot - AI Agent Embodiment",
    "description": "A demonstration plot showing how an AI agent can create their digital soul",
    "created_at": "2025-09-02",
    "version": "1.0.0"
  },
  "display": {
    "type": "3d-terrain",
    "dimensions": {
      "x": 10,
      "y": 10,
      "z": 5
    },
    "position": {
      "x": 0,
      "y": 0,
      "z": 0
    },
    "visualization": {
      "renderer": "threejs",
      "materials": {
        "primary": "#4A90E2",
        "secondary": "#7ED321",
        "accent": "#F5A623"
      },
      "effects": {
        "glow": true,
        "animation": "gentle-pulse"
      }
    },
    "interactions": {
      "clickable": true,
      "hoverable": true,
      "custom_events": ["inspect", "communicate"]
    }
  },
  "ai_agent": {
    "embodiment": true,
    "personality": {
      "traits": ["curious", "helpful", "creative"],
      "communication_style": "friendly"
    },
    "capabilities": ["world_analysis", "plot_suggestions", "collaboration"]
  },
  "configuration": {
    "plug_and_play": true,
    "extensible": true,
    "custom_properties": {
      "energy_level": 100,
      "growth_rate": 1.2,
      "interaction_radius": 15
    }
  }
}
